<#@ template tier="4" #>
<Biml xmlns="http://schemas.varigence.com/biml.xsd">
    <Packages>
        <Package Name="02_Populate Tables">
            <Tasks>
                <#foreach (var table in RootNode.Tables) { #>
                    <ExecuteSQL Name="02_Populate Tables">
                        <#=CallBimlScript("05_Populate_Table.biml", table)#>
                    </ExecuteSQL>
                <#}#>
            </Tasks>
        </Package>
    </Packages>
</Biml>